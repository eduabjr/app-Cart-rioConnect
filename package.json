{
  "name": "cartorio-connect",
  "version": "1.0.0",
  "description": "CartórioConnect - Aplicativo React Native com Expo para consultar dados de contato de cartórios interligados do Brasil",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android",
    "build:android:test": "eas build --platform android --profile preview",
    "build:ios": "eas build --platform ios",
    "build:ios:test": "eas build --platform ios --profile preview",
    "push": "powershell -ExecutionPolicy Bypass -File ./push.ps1",
    "push:quick": "powershell -ExecutionPolicy Bypass -File ./push.ps1",
    "pull": "powershell -ExecutionPolicy Bypass -File ./pull.ps1"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/cli-tools": "^20.0.2",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "expo": "~54.0.0",
    "expo-clipboard": "~8.0.8",
    "expo-linking": "~8.0.10",
    "expo-location": "~19.0.8",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-google-mobile-ads": "^16.0.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-webview": "13.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~19.1.10",
    "babel-preset-expo": "~54.0.0",
    "typescript": "^5.1.3"
  },
  "private": true
}
